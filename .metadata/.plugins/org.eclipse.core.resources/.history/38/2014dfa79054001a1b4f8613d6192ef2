package Application;

public class ExceptionScope extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	//Constructor
	private Exception ExceptionOutOfBoundsException;
	public ExceptionScope(Exception ExceptionOutOfBoundsException){
		//super("Le Numero d'Identification saisi n'existe pas dans la liste des Chauffeurs de notre Compagnie");
		if(ExceptionOutOfBoundsException == true) {}
		this.ExceptionOutOfBoundsException = ExceptionOutOfBoundsException;
		
	}
}
