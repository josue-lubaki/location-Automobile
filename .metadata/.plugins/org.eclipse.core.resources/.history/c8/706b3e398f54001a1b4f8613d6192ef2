package Application;

import java.io.PrintStream;

public class ExceptionScope extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	//Constructor
	public ExceptionScope(){
		super();
	}
	public String getMessage(PrintStream out) {
		String output = "Le Numero d'Identification saisi n'existe pas dans la liste des Chauffeurs de notre Compagnie";
		return output;
	}

}
